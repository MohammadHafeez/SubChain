@{
    ViewBag.Title = "Home";
}

<header>
    <img src="~/images/logo.svg" alt="" />
    <div class="burger">
        <div class="bar1"></div>
        <div class="bar2"></div>
        <div class="bar3"></div>
    </div>
    <nav class="cust__nav">
        <ul>
            <li><a href="~/Distributor/Index">Home</a></li>
            <li><a href="~/Distributor/Invoice">Invoices</a></li>
            <li><a href="~/Communication/Index">Communication</a></li>
            <li><a href="~/Home/SignOut">Sign Out</a></li>
            <li><a href="~/Distributor/CreateInv">Create</a></li>
        </ul>
    </nav>
</header>



<section>
    <style>
        table, th, td {
            border: 1px solid black;
            border-collapse: collapse;
        }

            table.center {
                margin-left: auto;
                margin-right: auto;
            }
    </style>
    <div class="table-responsive">
        <table id="viewStaff" class="table table-striped table-bordered">
            <thead class="thead-dark">
                <tr>
                    <th>Supplier ID</th>
                    <th>Date </th>


                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.SupplierID.ToString()</td>
                        <td>@item.IDate</td>

                    </tr>
                }
            </tbody>
        </table>
    </div>
</section>

